<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gov.alaska.gmc_handheld_v2_simpleJSON">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="gov.alaska.gmc_handheld_v2_simpleJSON.BaseActivity"></activity>
        <activity android:name="gov.alaska.gmc_handheld_v2_simpleJSON.Audit" />

        <activity android:name="gov.alaska.gmc_handheld_v2_simpleJSON.AddInventory" />

        <activity android:name="gov.alaska.gmc_handheld_v2_simpleJSON.Recode" />

        <activity android:name="gov.alaska.gmc_handheld_v2_simpleJSON.Move" />

        <activity android:name="gov.alaska.gmc_handheld_v2_simpleJSON.Summary" />

        <activity android:name="gov.alaska.gmc_handheld_v2_simpleJSON.Help" />

        <activity android:name="gov.alaska.gmc_handheld_v2_simpleJSON.AddContainer" />

        <activity
            android:name="gov.alaska.gmc_handheld_v2_simpleJSON.Lookup"
            android:theme="@style/Theme.AppCompat.DayNight"
            android:parentActivityName=".GetBarcode"
           />

        <activity
            android:name="gov.alaska.gmc_handheld_v2_simpleJSON.GetBarcode"
            android:parentActivityName="gov.alaska.gmc_handheld_v2_simpleJSON.MainActivity" />

        <activity android:name="gov.alaska.gmc_handheld_v2_simpleJSON.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>